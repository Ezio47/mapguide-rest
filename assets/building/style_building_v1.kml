<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <StyleMap id="georest-building-style">
      <Pair>
        <key>normal</key>
        <styleUrl>#georest-building-style-normal</styleUrl>
      </Pair>
      <Pair>
        <key>highlight</key>
        <styleUrl>#georest-building-style-highlight</styleUrl>
      </Pair>
    </StyleMap>
    <Style id="georest-building-style-normal">
      <IconStyle>
        <color>ff3af8ff</color>
        <scale>0.6</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/target.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
        <scale>0</scale>
      </LabelStyle>
      <LineStyle>
        <color>ff24ffff</color>
        <width>4</width>
      </LineStyle>
      <PolyStyle>
        <color>00ffffff</color>
      </PolyStyle>
      <BalloonStyle>
        <bgColor>fff6efe4</bgColor>
        <textColor>ff784c2a</textColor>
        <text><![CDATA[
      <h1>$[name]</h1>
        <table bgcolor="#ffffff" cellspacing="0" cellpadding="0" width="100%">
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
          <tr>
            <td><b>$[property/FeatId/displayName]</b>:</td>
            <td>$[property/FeatId]</td>
          </tr>
          <tr>
            <td><b>$[property/FMTADDRESS/displayName]</b>:</td>
            <td>$[property/FMTADDRESS]</td>
          </tr>
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
      <tr>
        <td colspan="2"><a href="http://localhost/mapguide/rest/data/property/$[property/FeatId].html">Building report for: $[name]</a></td>
      </tr>
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
        </table>
        ]]></text>
      </BalloonStyle>
    </Style>
    <Style id="georest-building-style-highlight">
      <IconStyle>
        <color>ff3af8ff</color>
        <scale>0.7</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/target.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
        <scale>0.8</scale>
      </LabelStyle>
      <LineStyle>
        <color>ff24ffff</color>
        <width>4</width>
      </LineStyle>
      <PolyStyle>
        <color>5f4080ff</color>
      </PolyStyle>
      <BalloonStyle>
        <bgColor>fff6efe4</bgColor>
        <textColor>ff784c2a</textColor>
        <text><![CDATA[
      <h1>$[name]</h1>
        <table bgcolor="#ffffff" cellspacing="0" cellpadding="0" width="100%">
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
          <tr>
            <td><b>$[property/FeatId/displayName]</b>:</td>
            <td>$[property/FeatId]</td>
          </tr>
          <tr>
            <td><b>$[property/FMTADDRESS/displayName]</b>:</td>
            <td>$[property/FMTADDRESS]</td>
          </tr>
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
      <tr>
        <td colspan="2"><a href="http://localhost/mapguide/rest/data/property/$[property/FeatId].html">Building report for: $[name]</a></td>
      </tr>
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
        </table>
        ]]></text>
      </BalloonStyle>
    </Style>
  </Document> 
</kml>