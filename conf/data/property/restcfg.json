{
    "Source": {
        "Type": "MapGuide",
        "FeatureSource": "Library://Samples/Sheboygan/Data/Parcels.FeatureSource",
        "FeatureClass": "SHP_Schema:Parcels"
    },
    "Representations": {
        "xml": {
            "Adapter": "FeatureSetXml",
            "Methods": {
                "GET": {
                    "MaxCount": 500
                }
            }
        },
        "csv": {
            "Adapter": "FeatureSetCsv",
            "Methods": {
                "GET": {
                    "MaxCount": 500
                }
            }
        },
        "atom": {
            "Adapter": "Template",
            "Methods": {
                "GET": {
                    "PageSize": 20,
                    "MaxCount": 100,
                    "TransformTo": "LL84",
                    "MimeType": "application/atom+xml",
                    "Templates": {
                        "Single": "property_atom_single.tpl",
                        "Many": "property_atom_many.tpl",
                        "None": "property_atom_none.tpl",
                        "Error": "property_atom_error.tpl"
                    }
                }
            }
        },
        "kml": {
            "Adapter": "Template",
            "Methods": {
                "GET": {
                    "PageSize": 20,
                    "MaxCount": 500,
                    "TransformTo": "LL84",
                    "MimeType": "application/vnd.google-earth.kml+xml",
                    "Templates": {
                        "Single": "property_kml_single.tpl",
                        "Many": "property_kml_many.tpl",
                        "None": "property_kml_none.tpl",
                        "Error": "property_kml_error.tpl"
                    }
                }
            }
        },
        "html": {
            "Adapter": "Template",
            "Methods": {
                "GET": {
                    "PageSize": 20,
                    "MaxCount": 500,
                    "MimeType": "text/html",
                    "Templates": {
                        "Single": "property_html_single.tpl",
                        "Many": "property_html_many.tpl",
                        "None": "property_html_none.tpl",
                        "Error": "property_html_error.tpl"
                    }
                }
            }
        },
        "png": {
            "Adapter": "MapImage",
            "Methods": {
                "GET": {
                    "MapDefinition": "Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition",
                    "SelectionLayer": "Parcels",
                    "MaxCount": 100
                }
            }
        },
        "png8": {
            "Adapter": "MapImage",
            "Methods": {
                "GET": {
                    "MapDefinition": "Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition",
                    "SelectionLayer": "Parcels",
                    "MaxCount": 100,
                    "ImageFormat": "PNG8"
                }
            }
        },
        "gif": {
            "Adapter": "MapImage",
            "Methods": {
                "GET": {
                    "MapDefinition": "Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition",
                    "SelectionLayer": "Parcels",
                    "MaxCount": 100,
                    "ImageFormat": "GIF"
                }
            }
        },
        "jpg": {
            "Adapter": "MapImage",
            "Methods": {
                "GET": {
                    "MapDefinition": "Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition",
                    "SelectionLayer": "Parcels",
                    "MaxCount": 100,
                    "ImageFormat": "JPG"
                }
            }
        },
        "geojson": {
            "Adapter": "FeatureSetGeoJson",
            "Methods": {
                "GET": {
                    "MaxCount": 500
                }
            }
        }
    }
}