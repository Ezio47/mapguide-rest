{
    "Source": {
        "Type": "MapGuide",
        "FeatureSource": "Library://Samples/Sheboygan/Data/Parcels.FeatureSource",
        "FeatureClass": "SHP_Schema:Parcels"
    },
    "Representations": {
        "xml": {
            "Adapter": "FeatureSetXml",
            "Methods": {
                "GET": {
                    "MaxCount": 500
                }
            }
        },
        "html": {
            "Adapter": "Template",
            "Methods": {
                "GET": {
                    "PageSize": 20,
                    "MaxCount": 500,
                    "MimeType": "text/html",
                    "Templates": {
                        "Single": "property_single.tpl",
                        "Many": "property_many.tpl",
                        "None": "property_none.tpl",
                        "Error": "property_error.tpl"
                    }
                }
            }
        },
        "png": {
            "Adapter": "MapImage",
            "Methods": {
                "GET": {
                    "MapDefinition": "Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition",
                    "SelectionLayer": "Parcels",
                    "MaxCount": 100
                }
            }
        },
        "geojson": {
            "Adapter": "FeatureSetGeoJson",
            "Methods": {
                "GET": {
                    "MaxCount": 500
                }
            }
        }
    }
}